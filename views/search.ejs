<div class="jumbotron">
        <h1 class="mt-4">Dashboard</h1>
        <p class="lead mb-3">
            <h3>
                Available Books
            </h3>
        <% users_with_books.forEach(function (user_with_book) { %>
                <ul>
                    <h5><b><%= user_with_book[0].name%></b> --- <%= user_with_book[0].email%></h5>
                    <% user_with_book[1].forEach(function (book) { %>
                    <ul>
                        <%= book%>
                    </ul>
                    <% }); %>
                </ul>
            <% }); %>
        <br>
        <a href="/dashboard" class="btn btn-secondary" >Dashboard</a>
        <br>
        <br>
        <a href="/donated" class="btn btn-secondary" >My Donated Books</a>
        <br>
        <br>
        <a href="/users/logout" class="btn btn-secondary" >Logout</a>
        <br>
        <br>
</div>